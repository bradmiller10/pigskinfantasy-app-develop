{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "android": "expo start --android --dev",
    "ios": "expo start --ios --dev",
    "web": "expo start --web --dev",
    "storybook": "start-storybook -p 19001",
    "storybook:web": "STORYBOOK=true expo start --web --dev",
    "storybook:android": "STORYBOOK=true expo start --android --dev",
    "storybook:ios": "STORYBOOK=true expo start --ios --dev",
    "test": "node_modules/.bin/jest"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/__mocks__/globalMock.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint"
  },
  "dependencies": {
    "@expo/vector-icons": "^12.0.0",
